{
  "name": "gatsby-source-teamtailor",
  "version": "0.3.0",
  "description": "Gatsby source plugin for building websites using Teamtailor as a data source",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrickedqvist/gatsby-source-teamtailor.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "author": "Patrick Edqvist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patrickedqvist/gatsby-source-teamtailor/issues"
  },
  "homepage": "https://github.com/patrickedqvist/gatsby-source-teamtailor#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "gatsby-node-helpers": "^0.3.0",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "babel-preset-gatsby-package": "^0.7.0",
    "cross-env": "^7.0.2"
  },
  "peerDependencies": {
    "gatsby": "^2.27.2"
  }
}
